function sortArrayByDate(array, dateKey) {
  array.sort(function(a, b) {
    let dateA = new Date(a[dateKey]);
    let dateB = new Date(b[dateKey]);
    return dateA - dateB;
  });

  return array;
}

function removeCharacters(str) {
  let modifiedStr = str.replace(/["\[\]]/g, '');
  if(modifiedStr.includes("")){

